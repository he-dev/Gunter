{
  "Variables": [
    {
      "Id": "locals",
      "Merge": "_Gunter#defaults",
      "Dictionary": {
        "ProductName": "Gunter",
        "Greeting-1": "Hi there!"
      }
    }
  ],
  "Logs": [
    {
      "$t": "TableOrView",
      "Id": "everything",
      "Merge": "_Gunter#everything"
    }
  ],
  "Tests": [
    {
      "Id": "any",
      "Enabled": true,
      "Level": "Fatal",
      "Message": "There is no test data.",
      "Check": [
        "everything"
      ],
      "Assert": "Count([_id]) > 0",
      "When": {
        "Passed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": []
    },
    {
      "Id": "must-not-run",
      "Enabled": false,
      "Level": "Error",
      "Message": "This test must not run.",
      "Check": [
        "everything"
      ],
      "Assert": "Count([_id]) = 0",
      "When": {
        "Failed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": []
    },
    {
      "Id": "last-one",
      "Enabled": true,
      "Level": "Debug",
      "Message": "There are less than 100 items.",
      "Check": [
        "everything"
      ],
      "Assert": "Count([_id]) < 100",
      "When": {
        "Failed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": []
    }
  ],
  "Messengers": [
    {
      "$t": "Mailr",
      "Id": "default-email",
      "Merge": "_Gunter#default-email"
    }
  ],
  "Reports": [
    {
      "Id": "full",
      "Merge": "_Gunter#full-report"
    }
  ]
}