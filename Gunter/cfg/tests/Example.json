{
  "Variables": [
    {
      "Id": "locals",
      "Merge": "_Gunter#defaults",
      "Items": {
        "ProductName": "Gunter",
        "Greeting-1": "Hi there!"
      }
    }
  ],
  "Logs": [
    {
      "$g": "TableOrView",
      "Id": "everything",
      "Merge": "_Gunter#everything"
    },
    {
      "$g": "TableOrView",
      "Id": "empty",
      "Merge": "_Gunter#empty"
    }
  ],
  "Tests": [
    {
      "Id": "fails",
      "Enabled": true,
      "Level": "Fatal",
      "Message": "There is no data.",
      "Check": [
        "empty"
      ],
      "Assert": "Count([_id]) > 0",
      "When": {
        "Failed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": [
        "error"
      ]
    },
    {
      "Id": "passes",
      "Enabled": true,
      "Level": "Information",
      "Message": "There is some data.",
      "Check": [
        "everything"
      ],
      "Assert": "Count([_id]) = 5",
      "When": {
        "Passed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": [
        "status"
      ]
    },
    {
      "Id": "must-not-run",
      "Enabled": false,
      "Level": "Error",
      "Message": "This test must not run.",
      "Check": [
        "everything"
      ],
      "Assert": "Count([_id]) = 0",
      "When": {
        "Failed": [
          "send -report full -use default-email",
          "halt"
        ]
      },
      "Tags": []
    }
  ],
  "Messengers": [
    {
      "$g": "Mailr",
      "Id": "default-email",
      "To": [
        "gunter@test.com"
      ],
      "Merge": "_Gunter"
    }
  ],
  "Reports": [
    {
      "Id": "full",
      "Merge": "_Gunter"
    }
  ]
}